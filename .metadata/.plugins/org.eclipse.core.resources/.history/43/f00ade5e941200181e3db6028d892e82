#ifndef TENSORBASE_H_
#define TENSORBASE_H_

namespace BC {


	template<class scalar, int order, class ml>
	struct TensorBase {


		struct ScalarShape {
		};
		struct VectorShape {
			int m;
		};
		struct MatrixShape {
			int m;
			int n;
		};

		scalar* array;

		template<class... integers>
		TensorBase() {

		}






	};











}



#endif /* TENSORBASE_H_ */
