#ifndef TENSORBASE_H_
#define TENSORBASE_H_

#include "Expression.h"
#include "Expression_Binary_Functors.cu"

namespace BC {


template<int order>
struct Shape {

};
template<>
struct Shape<0> {

};
template<>
struct Shape<1> {
	int m;
};

template<class scalar, int order, class ml>
struct TensorBase {
	int m;
	int n;
	int sz;

	scalar* array;


};

}

#endif /* TENSORBASE_H_ */
