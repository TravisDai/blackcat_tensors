#ifndef TENSORBASE_H_
#define TENSORBASE_H_

namespace BC {

template<int order>
struct Shape {

};
template<>
struct Shape<0> {

};
template<>
struct Shape<1> {
	int m;
};

template<class scalar, int order, class ml>
struct TensorBase: Shape {
	int m;
	int n;
	int sz;
};

}

#endif /* TENSORBASE_H_ */
