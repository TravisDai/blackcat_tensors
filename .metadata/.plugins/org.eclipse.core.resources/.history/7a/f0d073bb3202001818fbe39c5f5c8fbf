#ifndef UNARY_EXPRESSION_TRANSPOSE_IMPL_CU_
#define UNARY_EXPRESSION_TRANSPOSE_IMPL_CU_


#include "../BlackCat_Internal_Definitions.h" //__BC_gcpu__

#include <iostream>
namespace BC {
		struct transpose {

		template<class lv, class rv> __BC_gcpu__
		inline __attribute__((always_inline)) auto operator ()(lv& l, rv r) const {
			return (l = r);
		}
	};

}


#endif /* UNARY_EXPRESSION_TRANSPOSE_IMPL_CU_ */
