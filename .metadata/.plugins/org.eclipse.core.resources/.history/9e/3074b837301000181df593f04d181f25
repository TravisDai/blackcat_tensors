#ifndef TUPLE_H_
#define TUPLE_H_


//class... tuple --> means some unknown number of types


//Assume this tuple is a size_of 0

template<class derived, class list...>
class Tuple_Array {
	using P = derived;
	using N = Tuple_Array<list...>;

}




template<class... list>
class Tuple {};
