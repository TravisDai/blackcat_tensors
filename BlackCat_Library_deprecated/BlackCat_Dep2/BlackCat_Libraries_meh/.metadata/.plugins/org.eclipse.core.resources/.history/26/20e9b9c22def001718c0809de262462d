#include <iostream>
#include "BlackCat_Tensors.h"
#include "BC_Internals/BC_Core/Vector.h"

template<int sz>
struct sizer {
		static constexpr int size = sz;
};

using BC::Vector;

void method() {}

int main() {
	method(0);

	Vector<double> a(5);
	Vector<double> b(5);
	Vector<double> c(5);

	a.init();
	b.init();
	c.init();

	b.inc();
	c.inc();

	b.print();

	a = b;
	a.print();

	a = b + c;

	a.print();

	std::cout << " out" <<std::endl;
}
