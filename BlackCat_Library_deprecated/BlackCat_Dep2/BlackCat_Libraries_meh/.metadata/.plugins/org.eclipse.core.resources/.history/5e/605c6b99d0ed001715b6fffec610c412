#include <iostream>
#include "BlackCat_Tensors.h"
#include "BC_Internals/BC_Core/Tensor_Core.h"
#include "BC_Internals/BC_Core/Vector.h"

template<int sz>
struct sizer {
		static constexpr int size = sz;
};

using BC::Vector;

int main() {


	Vector<double, sizer<5>> a;
	Vector<double, sizer<5>> b;
	Vector<double, sizer<5>> c;

	a.init();
	b.init();
	c.init();

	b.inc();
	c.inc();

	b.print();

	a =b;
	a.print();

	a = b + c;

	a.print();


	std::cout << " out" <<std::endl;
}
