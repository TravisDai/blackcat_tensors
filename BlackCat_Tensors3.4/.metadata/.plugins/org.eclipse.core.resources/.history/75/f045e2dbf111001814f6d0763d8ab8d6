#ifndef BC_CORE_H_
#define BC_CORE_H_

namespace BC {

	template<class derived, class T, class ml>
	class TensorBase  {

		operator const derived&() { return static_cast<derived&>(*this); }
		operator 	   derived&() { return static_cast<derived&>(*this); }

	};

}


#endif /* BC_CORE_H_ */
